<!-- Create your form here -->
<h1>Student Form</h1>

<form>
  <%= form_tag students_path do %>

    <label>First Name</label>
    <div>
      <%= text_field_tag 'first_name'%>
    </div><br>

    <label>Last Name</label>
    <div>
      <%= text_field_tag 'last_name'%>
    </div><br>

    <%= submit_tag "Go", :name => nil %>
  <% end %>
</form>
